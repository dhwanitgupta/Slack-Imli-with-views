<% provide(:title, @channel.title) %>
<h1><%=@channel.title%></h1>
<%= link_to "Add User", add_user_to_channel_new_path(:id => @channel.id) %> 
<ul class="users">
  <% for user in @users %>
    <div class="row">
      <aside class="col-md-4">
        <section class="user_info">
          <h1>
            <%= gravatar_for user %>
            <%= user.name %>
          </h1>
        </section>
      </aside>
    </div>
  <%end%>
</ul>